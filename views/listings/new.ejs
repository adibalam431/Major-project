<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet"
  integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH" crossorigin="anonymous">
<div class="row mt-3">
  <div class="col-8 offset-2 ">
    <h3>Create New Listing</h3>
    <form method="POST" action="/listings" novalidate class="needs-validation" enctype="multipart/form-data">

      <div class="mb-3">
        <lable for="title" class="form-lebel">Title</lable>
        <input name="list[title]" placeholder="Title" type="text" class="form-control" required />
        <div class="valid-feedback">
          Looks good!
        </div>
        <div class="invalid-feedback">
          Title required
        </div>
      </div>
      <div class="mb-3">
        <lable for="description" class="form-lable">Description</lable>
        <textarea name="list[description]" class="form-control"></textarea>
      </div>


      <!-- <div class="mb-3">
        <lable for="image" class=" form-lable">img link</lable>
        <input name="list[image]" placeholder="Image Link" type="text" class="form-control" />
      </div> -->

      <div class="mb-3">
        <lable for="image" class=" form-lable">img link</lable>
        <input name="list[image]" placeholder="Image Link" type="file" class="form-control" required/>
      </div>

      <div class="row">
        <div class="mb-3 col-md-4">
          <label for="price" class="form-lable">price</label>
          <input name="list[price]" placeholder="999" type="number" class="form-control" required />
          <div class="valid-feedback">
            Looks good!
          </div>
          <div class="invalid-feedback">
            Invalid price
          </div>
        </div>

        <div class="mb-3 col-md-8">
          <lable for="country" class="form-lable">country</lable>
          <input name="list[country]" placeholder="India" type="text" class="form-control" required />
          <div class="valid-feedback">
            Looks good!
          </div>
          <div class="invalid-feedback">
            Country not defined
          </div>
        </div>
      </div>

      <div class="mb-3">
        <lable for="location" class="form-lable">Location </lable>
        <input name="list[location]" placeholder="Ranchi" type="text" class="form-control" required />
        <div class="valid-feedback">
          Looks good!
        </div>
        <div class="invalid-feedback">
          Location not defined
        </div>
      </div>

      <button class="btn btn-dark add-btn mt-3">Add</button>
    </form>

  </div>
</div>
<script src="/js/script.js"></script>